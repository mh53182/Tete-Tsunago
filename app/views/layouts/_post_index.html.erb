<% posts.each do |post| %>
  <div class="post_card">
    <%#= image_tag post.post_image %>
    <%= post.body %>
    <%= post.category_i18n %>
    <%= post.child.nickname %>
    <% if post.user == current_user %>
      <%= link_to "編集", edit_post_path(post.id) %>
      <%= link_to "削除", post_path(post.id), method: :delete, 'data-confirm' => "本当に削除しますか？" %>
    <% end %>
  </div>
<% end %>